{
    "name": "nswdpc/silverstripe-filetype-management",
    "description": "Allow management of configured allowed file types in the administration area and on a per-upload field basis.",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "nsw",
        "module",
        "file",
        "mimetypes",
        "userforms"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "James",
            "role": "Developer"
        }
    ],
    "require": {
        "silverstripe/siteconfig" : "^6",
        "symbiote/silverstripe-multivaluefield": "^7 || dev-7"
    },
    "require-dev": {
        "cambis/silverstripe-rector": "^2",
        "cambis/silverstan": "^2",
        "friendsofphp/php-cs-fixer": "^3",
        "phpunit/phpunit": "^11.5",
        "phpstan/phpstan": "^2",
        "rector/rector": "^2",
        "nswdpc/ci-files": "dev-v-4",
        "vlucas/phpdotenv": "^5",
        "phpstan/phpstan-phpunit": "^2"
    },
    "suggest": {
        "silverstripe/userforms": "For EditableFileField support"
    },
    "replace": {
        "nswdpc/silverstripe-editablefilefield-filetypes": "self.version"
    },
    "autoload": {
        "psr-4": {
            "NSWDPC\\FileTypeManagement\\": "src/",
            "NSWDPC\\FileTypeManagement\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/vendor-plugin": true,
            "silverstripe/recipe-plugin": true,
            "phpstan/extension-installer": true
        }
    }
}
